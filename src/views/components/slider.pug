		.carousel-item
			img(src='http://sprayplast.ru//images/works/8/1.jpg', alt='...').d-block.w-100
		.carousel-item.active
			img(src='http://sprayplast.ru//images/works/8/2.jpg', alt='...').d-block.w-100
		.carousel-item
			img(src='http://sprayplast.ru//images/works/8/3.jpg', alt='...').d-block.w-100
		.carousel-item
			img(src='http://sprayplast.ru//images/works/8/4.jpg', alt='...').d-block.w-100
